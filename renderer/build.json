{
  "outputDir": "../dist/assets",
  "publicPath": "./",
  "customWebpack": true,
  "plugins": [
    [
      "build-plugin-fusion",
      {
        "themePackage": "@alifd/theme-design-pro"
      }
    ],
    [
      "build-plugin-moment-locales",
      {
        "locales": [
          "zh-cn"
        ]
      }
    ],
    "build-plugin-css-assets-local",
    ["build-plugin-webpack5", {
      "remoteRuntime": true
    }],
    "./build.plugin.js"
  ]
}
