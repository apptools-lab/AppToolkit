{
  "name": "AppToolkit",
  "version": "1.0.0",
  "private": true,
  "main": "packages/main/build/index.cjs",
  "scripts": {
    "prepare": "husky install",
    "start": "icejs start --disable-open --rootDir ./packages/renderer",
    "prebuild": "icejs build --rootDir ./packages/renderer",
    "build": "NPM_CONFIG_ELECTRON_MIRROR=https://npmmirror.com/mirrors/electron/ electron-builder build --config electron-builder.config.js",
    "test": "vitest --config ./vitest.config.ts",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint --ext .ts,.tsx,.js,.jsx ./",
    "eslint:fix": "eslint --ext .ts,.tsx,.js,.jsx ./ --fix",
    "stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,less}\" --fix"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.2",
    "@ant-design/pro-layout": "^6.5.1",
    "antd": "^4.0.0",
    "consola": "^2.15.3",
    "cross-spawn": "^7.0.3",
    "electron-updater": "^4.6.1",
    "execa": "^6.1.0",
    "fs-extra": "^10.0.1",
    "globby": "^13.1.1",
    "jquery": "^3.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-slick": "^0.29.0",
    "simple-git": "^3.5.0",
    "slick-carousel": "^1.8.1",
    "ssh-config": "^4.1.4",
    "ssh-keygen": "^0.5.0"
  },
  "devDependencies": {
    "@applint/spec": "^1.0.0",
    "@commitlint/cli": "^16.1.0",
    "@types/cross-spawn": "^6.0.2",
    "@types/node": "^17.0.2",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-slick": "^0.23.8",
    "build-plugin-antd": "^0.1.2",
    "build-plugin-ignore-style": "^0.1.0",
    "c8": "^7.11.0",
    "electron": "^18.0.0",
    "electron-builder": "^22.14.5",
    "eslint": "^8.0.0",
    "husky": "^7.0.4",
    "ice.js": "^2.4.2",
    "sass": "^1.50.0",
    "sass-loader": "^12.6.0",
    "stylelint": "^14.6.1",
    "typescript": "^4.5.4",
    "vitest": "^0.8.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apptools-lab/AppToolkit"
  }
}
